{
    "manifest_version": 3,
    "name": "yratings",
    "version": "1.0",
    "description": "RateMyProfessor on myBYU!",
    "permissions": [
      "activeTab"
    ],
  
    "background": {
      "service_worker": "sw.js",
      "persistent": false
    },
  
    "browser_action": {
      "default_popup": "popup.html"
    },
  
   "content_scripts": [
     {
       "matches": ["https://*.your-website.com/*"],
       "css": ["my-styles.css"],
       "js": ["content-script.js"]
     }
   ]
  }